<svg width="600" height="400" xmlns="http://www.w3.org/2000/svg">
  <rect width="600" height="400" fill="#2d3748"/>
  <rect x="20" y="20" width="560" height="360" fill="#1a202c" rx="8"/>
  
  <!-- Dashboard title -->
  <text x="50" y="60" font-family="sans-serif" font-size="18" fill="#f7fafc" font-weight="bold">Application Dashboard</text>
  
  <!-- Metrics cards -->
  <rect x="50" y="80" width="150" height="80" fill="#48bb78" rx="6" opacity="0.8"/>
  <text x="125" y="110" font-family="sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">CPU</text>
  <text x="125" y="140" font-family="sans-serif" font-size="24" fill="#ffffff" font-weight="bold" text-anchor="middle">23%</text>
  
  <rect x="220" y="80" width="150" height="80" fill="#489be4" rx="6" opacity="0.8"/>
  <text x="295" y="110" font-family="sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Memory</text>
  <text x="295" y="140" font-family="sans-serif" font-size="24" fill="#ffffff" font-weight="bold" text-anchor="middle">512MB</text>
  
  <rect x="390" y="80" width="150" height="80" fill="#fbb6ce" rx="6" opacity="0.8"/>
  <text x="465" y="110" font-family="sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Requests</text>
  <text x="465" y="140" font-family="sans-serif" font-size="24" fill="#ffffff" font-weight="bold" text-anchor="middle">1.2K/m</text>
  
  <!-- Graph -->
  <rect x="50" y="190" width="490" height="150" fill="#4a5568" rx="6" opacity="0.3"/>
  <text x="50" y="215" font-family="sans-serif" font-size="12" fill="#e2e8f0">Response Time (ms)</text>
  <polyline points="70,300 120,280 170,290 220,260 270,270 320,250 370,265 420,240 470,255 520,245" 
            stroke="#48bb78" stroke-width="3" fill="none"/>
  <line x1="60" y1="350" x2="540" y2="350" stroke="#e2e8f0" stroke-width="1"/>
  <line x1="60" y1="220" x2="60" y2="350" stroke="#e2e8f0" stroke-width="1"/>
</svg>
